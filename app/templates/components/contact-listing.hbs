<div class="list-group">
  {{#each model as |contact|}}
    {{#link-to "contacts.view" contact class="list-group-item"}}<strong>{{contact.name}}</strong><br/>{{contact.emailAddress}}<br/>{{contact.phoneNumber}}<br/>{{/link-to}}
  {{else}}
    <div class="alert alert-info">{{t 'contacts.no-results'}}</div>
  {{/each}}
</div>
